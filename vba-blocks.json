{
  "name": "vim_enhanced",
  "description": "Excel add-in that provides Vim-style keybindings for Excel.",
  "type": "Excel.AddIn",
  "version": "0.1.0",
  "sources": [
    {
      "path": "src",
      "includes": [
        "DefaultConfig.bas",
        "classes/*.cls",
        "core/*.bas",
        "functions/*.bas",
        "userforms/*.frm",
        "userforms/*.frx",
        "workbook/*.cls"
      ]
    }
  ],
  "references": [],
  "output": {
    "path": "vim_enhanced.xlam"
  }
}
